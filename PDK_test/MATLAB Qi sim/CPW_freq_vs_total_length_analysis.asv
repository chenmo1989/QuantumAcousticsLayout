% This script deals with the frequency vs total resonator length data from
% COMSOL simulation.

% All resonators have coupling_gap = 5um, coupling_length = 150um.

total_length = [5800,5700,5500];
freq = [4.6081,4.6824,4.8351];

% Polynomial degree
degree = 1;

% Fit polynomial
p = polyfit(total_length, freq, degree);

% Evaluate polynomial over a finer grid for smooth plotting
x_fit = linspace(min(x), max(x), 100);
y_fit = polyval(p, x_fit);

% Plot
figure;
plot(tota_length, , 'o', 'MarkerSize', 8, 'DisplayName', 'Data');
hold on;
plot(x_fit, y_fit, '-', 'LineWidth', 2, 'DisplayName', sprintf('Polyfit (degree %d)', degree));
